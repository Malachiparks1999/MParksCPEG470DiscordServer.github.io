{
    "rules": {
      "chats": {
        "$uid":{
          ".read": "auth.uid != null",
          ".write": "auth.uid != null && root.child('chats').child($uid).child('author').val() === auth.uid",
        }
      },
      "rules": {
        ".read": true,
        ".write": false
      }
    }
  }